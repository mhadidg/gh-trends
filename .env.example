# Email sending (disabled for development)
SEND_ENABLED=false

# GitHub API (optional for dev - uses mocks when not set)
GITHUB_TOKEN=ghp_your_token_here

# Buttondown API key (required when SEND_ENABLED=true)
BUTTONDOWN_API_KEY=your_buttondown_key_here

# Days to look back when searching for trending GitHub repos (optional, default is 7)
FETCH_WINDOW_DAYS=7

# Number of repos to feature in the newsletter (optional, default is 10)
NEWSLETTER_TOP_N=10
